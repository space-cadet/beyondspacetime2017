@article{Immirzi1996Real,
	Abstract = {Both real and complex connections have been used for canonical gravity: the complex connection has {SL}({2,C}) as gauge group, while the real connection has {SU}(2) as gauge group. We show that there is an arbitrary parameter \$\beta\$ which enters in the definition of the real connection, in the Poisson brackets, and therefore in the scale of the discrete spectra one finds for areas and volumes in the corresponding quantum theory. A value for \$\beta\$ could be could be singled out in the quantum theory by the Hamiltonian constraint, or by the rotation to the complex Ashtekar connection.},
